{
    "name": "slide-plane",
    "description": "A fast-paced side-scrolling airplane game built with Phaser 3, React, and TypeScript.",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/izep/slide-plane.git"
    },
    "author": "izep",
    "license": "MIT",
    "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
    "bugs": {
        "url": "https://github.com/izep/slide-plane/issues"
    },
    "homepage": "https://github.com/izep/slide-plane#readme",
    "keywords": [
        "game",
        "phaser",
        "phaser3",
        "react",
        "vite",
        "typescript",
        "side-scroller",
        "airplane-game"
    ],
    "scripts": {
        "dev": "node log.js dev & vite --config vite/config.dev.mjs",
        "build": "node log.js build & vite build --config vite/config.prod.mjs",
        "dev-nolog": "vite --config vite/config.dev.mjs",
        "build-nolog": "vite build --config vite/config.prod.mjs",
        "deploy": "gh-pages -d dist"
    },
    "dependencies": {
        "phaser": "^3.90.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.22.0",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.22.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "gh-pages": "^6.3.0",
        "globals": "^16.0.0",
        "terser": "^5.28.1",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.26.1",
        "vite": "^6.3.1"
    }
}
